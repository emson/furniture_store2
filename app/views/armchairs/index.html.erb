

<%= render('shared/header') %>

<main role="main">
  <section class="container inpage">
    <div class='clip'><%= image_tag(@primary_img, { id:'catimg', alt:'' }) %></div>
    <ul id='linklist'>
      <% @armchairs.each do |armchair| %>
        <li><%= link_to(armchair.name, armchair_path(armchair), { :'data-src' => asset_path(armchair.image_main) } ) %></li>
      <% end %>
    </div>
  </section>

  <%= render('shared/call_us') %>
</main>


